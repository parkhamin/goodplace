<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="l">
	<!-- 로그인을 위한 id체크 -->
	<select id="check_id" parameterType="String" resultType="signup">
		select * from signup where id=#{id}
	</select>
</mapper>












